{
  "name": "skix",
  "version": "1.0.0",
  "description": "Freeski Game",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "webpack": "webpack --mode development",
    "doc": "npx jsdoc -c ./jsDoc.json -t ./node_modules/ink-docstrap/template -R ./docs/README.md",
    "build": "npm run lint && npm run rm && npm run doc && npm run webpack",
    "rm": "rm -rf ./dist && mkdir dist/ && mkdir dist/css && mkdir dist/img && cp -a html_app/. dist  && cp -a css/. dist/css  && cp -a img/. dist/img",
    "lint": "eslint src --ext .js",
    "test": "npm run lint && npm run cy:run",
    "cy:run": "cypress run --spec \"cypress/integration/*\" --reporter mochawesome",
    "cy:open": "cypress open && cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web2solutions/skix.git"
  },
  "author": {
    "name": "Eduardo Almeida",
    "url": "https://github.com/web2solutions",
    "email": "web2solucoes@gmail.com"
  },
  "contributors": [
    "Dan Wash"
  ],
  "license": "",
  "homepage": "https://github.com/web2solutions/skix#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/node": "^7.2.2",
    "@babel/plugin-external-helpers": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-syntax-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@cypress/webpack-preprocessor": "^5.7.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^4.5.1",
    "ink-docstrap": "^1.3.2",
    "jsdoc-to-markdown": "^6.0.1",
    "webpack": "^5.14.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "core-js": "^2.6.12",
    "css-loader": "^2.1.1",
    "cypress": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "presets": [
    "@babel/preset-env"
  ]
}
